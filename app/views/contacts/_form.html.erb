<%= form_with(model: contact, local: true) do |form| %>
  <% if contact.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(contact.errors.count, "error") %> prohibited this contact from being saved:</h2>

      <ul>
        <% contact.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :contact_id %>
    <%= form.number_field :contact_id %>
  </div>

  <div class="field">
    <%= form.label :contact_name %>
    <%= form.text_field :contact_name %>
  </div>

  <div class="field">
    <%= form.label :contact_title %>
    <%= form.text_field :contact_title %>
  </div>

  <div class="field">
    <%= form.label :contact_detail_type %>
    <%= form.text_field :contact_detail_type %>
  </div>

  <div class="field">
    <%= form.label :contact_company %>
    <%= form.text_field :contact_company %>
  </div>

  <div class="field">
    <%= form.label :created_at %>
    <%= form.date_select :created_at %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
