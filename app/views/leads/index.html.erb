<p id="notice"><%= notice %></p>

<h1>Leads</h1>

<table>
  <thead>
    <tr>
      <th>Lead</th>
      <th>Lead company name</th>
      <th>Lead contact name</th>
      <th>Assigned by</th>
      <th>Created by</th>
      <th>Created at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.lead_id %></td>
        <td><%= lead.lead_company_name %></td>
        <td><%= lead.lead_contact_name %></td>
        <td><%= lead.assigned_by %></td>
        <td><%= lead.created_by %></td>
        <td><%= lead.created_at %></td>
        <td><%= link_to 'Show', lead %></td>
        <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
        <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lead', new_lead_path %>
